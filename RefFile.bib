@article {ridgelet,
	author = {Cand{\`e}s, Emmanuel J. and Donoho, David L.},
	title = {Ridgelets: a key to higher-dimensional intermittency?},
	volume = {357},
	number = {1760},
	pages = {2495--2509},
	year = {1999},
	doi = {10.1098/rsta.1999.0444},
	publisher = {The Royal Society},
	abstract = {In dimensions two and higher, wavelets can efficiently represent only a small range of the full diversity of interesting behaviour. In effect, wavelets are well adapted for point{\textendash}like phenomena, whereas in dimensions greater than one, interesting phenomena can be organized along lines, hyperplanes and other non{\textendash}point{\textendash}like structures, for which wavelets are poorly adapted. We discuss in this paper a new subject, ridgelet analysis, which can effectively deal with line{\textendash}like phenomena in dimension 2, plane{\textendash}like phenomena in dimension 3 and so on. It encompasses a collection of tools which all begin from the idea of analysis by ridge functions œà(u1x1 + ‚Éõ + unxn whose ridge profiles œà are wavelets, or alternatively from performing a wavelet analysis in the Radon domain. The paper reviews recent work on the continuous ridgelet transform (CRT), ridgelet frames, ridgelet orthonormal bases, ridgelets and edges and describes a new notion of smoothness naturally attached to this new representation.},
	issn = {1364-503X},
	URL = {http://rsta.royalsocietypublishing.org/content/357/1760/2495},
	eprint = {http://rsta.royalsocietypublishing.org/content/357/1760/2495.full.pdf},
	journal = {Philosophical Transactions of the Royal Society of London A: Mathematical, Physical and Engineering Sciences}
}
